<section class="achievement_details">
    <img class="profile" src="/static/images/people/{{person.picture}}" title="{{person.name}}" width="180" height="180" />
    <h3>{{person.name}}</h3>
    {{#parseDescription}}
    <p>{{achievement.description}} <a href="{{achievement.source}}">More</a></p>
    {{/parseDescription}}
    <a class="button all_achievements" href="#" title="See all achievements">See all achievements</a>
</section>
